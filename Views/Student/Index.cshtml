@model IEnumerable<myweb.Models.Student>
    @{
    ViewData["Title"] = "Index";
    }
<a asp-action="Create" asp-controller="Student"><button type="button" class="btn btn-primary">Add</button></a>
<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">StudentId</th>
            <th scope="col">Action</th>
        </tr>
    </thead>
    <tbody>

            @foreach (var item in Model)
            {
                
        <tr>
            
                <td scope="row"> @item.Id</td>
                <td> @item.Name</td>
                <td> @item.StudentId</td>
                <td><a asp-action="Edit" asp-controller="Student" asp-route-id=@item.Id><i class="bi bi-pencil-square"></i></a>
                   <a asp-action="Delete" asp-controller="Student" asp-route-id=@item.Id> <i class="bi bi-trash-fill"></i></a>
                </td>
               
            </tr>
        }
       
       
    </tbody>
</table>
 
</table>